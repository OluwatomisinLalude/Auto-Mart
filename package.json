{
  "name": "auto-mart",
  "version": "1.0.0",
  "description": "Auto Mart is an online marketplace for automobiles of diverse makes, model or body type where users can sell their cars or buy from trusted dealerships or private sellers.Â ",
  "main": "src/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha src/test --exit --no-timeouts",
    "clean:build": "rm -R dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel src --out-dir dist --source-maps",
    "start": "node dist/index.js",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "watch": "nodemon --exec babel-node --presets babel-preset-env src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OluwatomisinLalude/Auto-Mart.git"
  },
  "keywords": [
    "express",
    "automart",
    "node",
    "javascript"
  ],
  "author": "Oluwatomisin Lalude",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/OluwatomisinLalude/Auto-Mart/issues"
  },
  "homepage": "https://github.com/OluwatomisinLalude/Auto-Mart#readme",
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-mocha": "^5.3.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1"
  }
}
